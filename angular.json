{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-requests": {
      "projectType": "library",
      "root": "projects/angular-requests",
      "sourceRoot": "projects/angular-requests/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/angular-requests/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-requests/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/angular-requests/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/angular-requests/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "3ed4e2a6-c9ed-4b79-b441-686b982d9fce"
  }
}
